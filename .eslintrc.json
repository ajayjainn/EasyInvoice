{
    "env": {
        "browser": true,
        "node":true,
        "es2021": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi":["error","never"],
        "no-console":"off",
        "import/extensions": "off"
    }
}
